{
  "name": "react-redux-saga-universal-application",
  "version": "1.0.2",
  "description": "The simplest webpack starter kit for React",
  "main": "index.js",
  "scripts": {
    "start-dev-client": "webpack-dev-server --open --config ./webpack/webpack.dev.js",
    "start-dev-iso": "concurrently \"nodemon src/server.js\" \"webpack --config ./webpack/webpack.dev.iso.js\"",
    "build-prod-client": "webpack --config ./webpack/webpack.prod.js",
    "build-prod-server": "webpack --config ./webpack/webpack.prod.iso.js",
    "start-prod-iso": "node dist/server.bundle.js"
  },
  "keywords": [
    "es6",
    "react",
    "react-16",
    "sass",
    "webpack",
    "babel",
    "starter-kit",
    "redux",
    "redux-saga",
    "react-router-v4"
  ],
  "author": "gzoreslav@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "express": "^4.16.2",
    "lodash": "^4.17.4",
    "node-fetch": "^1.7.3",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "whatwg-fetch": "^2.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gzoreslav/react-redux-saga-universal-application.git"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "js jsx",
    "execMap": {
      "js": "babel-node --presets env,stage-2"
    }
  }
}
